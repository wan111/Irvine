//hint=OnStartThreadイベントのautoRenameA.dmsと一緒に使用してください
//match=comment:.*\{34686344-1C41-4E59-B201-6CFBFAF7101D\}=\d;
//author=wan
//path=program
//priority=900
//end
function(){
	item.comment = item.comment.replace(/{34686344-1C41-4E59-B201-6CFBFAF7101D}=(\d);/, '');
	switch(RegExp.$1){
		case '0':
			item.filename = '';
			break;
		case '1':
			item.filename = item.filename.replace(/\.irvpart$/, '');
			break;
	}
}
